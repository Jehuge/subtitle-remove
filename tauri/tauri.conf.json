{
  "productName": "Subtitle Remove",
  "version": "1.0.0",
  "identifier": "com.subtitle.remove",
  "build": {
    "beforeDevCommand": "cd ../front && npm run dev",
    "beforeBuildCommand": "cd ../front && npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../../dist",
    "withGlobalTauri": false
  },
  "app": {
    "windows": [
      {
        "title": "AI 图片去水印",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "../../python"
    ]
  },
  "plugins": {}
}

