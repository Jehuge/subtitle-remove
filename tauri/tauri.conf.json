{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Subtitle Remove",
  "version": "1.0.0",
  "identifier": "com.subtitle.remove",
  "build": {
    "beforeDevCommand": "sh tauri/start-frontend.sh",
    "beforeBuildCommand": "cd front && npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "AI 图片去水印",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "../python"
    ]
  },
  "plugins": {}
}

